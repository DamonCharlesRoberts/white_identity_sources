@article{blumer_1958_psr,
  author = {Herbert Blumer},
  year = 1958,
  title = {Race prejudice As A Sense of Group Position},
  journal = {The Pacific Sociological Review},
  volume = 1,
  number = 1,
  pages = {3--7},
  annotation = {
    - Presents the orginal realistic conflict theory
    - RCT:
      - prejudice is based on a sense of group position rather than a set of intergroup attitudes
      - It comes about as the group collectively sensing threat towards the group
  }
}
@article{bobo_hutchings_1996_asr,
  author = {Lawrence Bobo and Vincent L. Hutchings},
  year = 1996,
  title = {Perceptions of Racial Group Competition: Extending Blumer's Theory of Group Position to a Multiracial Social Context},
  journal = {American Sociological Review},
  volume = 61,
  number = 6,
  pages = {951--972},
  annotation = {
    RQ: Does feelings of racial alienation or opression make members of a racial group perceive members of other racial groups as larger threats?

    Argues:
    - Blumer's theory argues that there can be collective perceptions of a racial hierarchy where some are dominant groups and others are oppressed and alienated.
    - They argue that perceptions of those in other racial groups are driven by this hierarchy and an evaluation of changes to this social order.
      - That is, we may see changes to the social order as generating threat and therefore we will see the individuals in those racial groups as threatening.
    
    Methods:
    - Use data from the 1992 LA County Social survey
    
    Findings:
    - Whites' perceptions of Blacks as zero-sum competitors increase with increasing racial alienation, with negative stereotyping, social distance, and increasing income.
  }
}
%% Social identity theory
@incollection{ellemers_haslam_2012_htsp,
  author = {Naomi Ellemers and S. Alexander Haslam},
  year = 2012,
  title = {Social Identity Theory},
  booktitle = {Handbook of Theories of Social Psychology},
  editors = {Paul A. M. Van Lange and Arie W. Kruglanski and E. Tory Higgins},
  publisher = {Sage Publications Ltd.},
  volume = 2,
  address = {London},
  pages = {379--398},
  annotation = {
    - Provides an excellent literature review and historical account of social identity theory, how it compares and contrasts to other major theories about identity and how it operates.
    - Social identity theory is primarily a theory about how us versus them can happen. It is a theory of delineation and the varied social contexts in which identities can form, be salient, and shape attitudes and behaviors.
    - SIT sees social context as the key determinant.
    - It first came about from now famous experiments called the mere categorization effect.
      - the theory is not this idea alone. The results from these studies demonstrated that social identities can appear with categorization regardless of competition.
    - The findings here were at odds with RCT which suggests that conflicts between members of groups happen because of competition over resources which would expect that mere categorization cannot happen since it doesn't have pre-established groups that are competing for resources.
    - There are three key principles to SIT:
      - there are psychological processes that explain how social identities are differnt than personal identities
        - because the self is implicated in the group, people are motivated to emphsize and secure the ways in which their group is distinct from others and homogenous within the group.
      - distinguishes between different strategies people use to derive a positive social identity.
      - specifies the key characteristics of the social structurethat dtermine which strategy is most likely to be used.
    - Key predictions of the theory
      - individuals internalize group membership as a meaningful aspect of their self-concept. They will try to make favorable comparisions between this group and relevant outgroups.
      - Social categorization is sufficient to engender intergroup discrimination and inter-group conflict
      - when searching for positive social identity, it can take different forms depending on the beliefs of social reality that are used to justify the group
    - Self-categorization theory (SCT) built upon SIT.
      - the theory argues that self-categorization can happen at different levels of inclusiveness and abstractions (Londoner, UK citizen, European, Human, etc.)
  }
}

%% Self-categorization theory
@article{ellemers_et-al_2002_arp,
  author = {Naomi Ellemers and Russell Spears and Bertjan Doosje},
  year = 2002,
  title = {Self and Social Identity},
  journal = {Annual Review of Psychology},
  volume = 53,
  pages = {161--186},
  annotation = {
    RQ: What conditions effect the degree to which one reports an identity as central to their self-conception?

    Argues:
    - Uses SIT to argue that group commitment and features of the social context guide the centrality of identity.
    - 1. No threat, low group commitment: leads to noninvolvement
    - 2. No threat, high commitment: leads to identity expression
    - 3. self-directed threat, low commitment: leads to self-affirmation and less expressed desire to be percieved as a group.
    - 4. self-directed threat, high commitment: leads to negative affect and a strong desire to be accepted by the group rather than a desire to find group heterogeneity.
    - 5. group-directed threat, low commitment: leads to negative affective reactions like guilt and to look for ways to distance themselves from the group and to recognize the group’s harm.
    - 6. group-directed threat, high commitment: leads to a desire to protect the group and encourages behavioral responses like group differentiation from other groups and for group affirmation
  }
}
@incollection{turner_reynolds_2012_htsp,
  author = {John C. Turner and Katherine J. Reynolds},
  year = 2012,
  title = {Self-Categorization Theory},
  booktitle = {Handbook of Theories of Social Psychology},
  editors = {Paul A. M. Van Lange and Arie W. Kruglanski and E. Tory Higgins},
  publisher = {Sage Publications Ltd.},
  volume = 2,
  address = {London},
  pages = {399--417},
  annotation = {
    - Provides an extremely good literature review of the history of self-categorization theory, how it compares to social identity theory, how it came to be, and what its place is in relation to the other major theories of identity.
    - SCT is an extension upon SIT. In many ways it has similar predictions.
    - It outlines how people can be both individuals and a group member with a number of different identities that can be had. It predicts how and when people define themselves as an individual and as a bgroup. 
    - A key implication of SCT is that a self-category is not just some fixed, cognitive structure in a mental system waiting to be activated. Rather it is a categorization that responds to a dynamic and variable world. 
    - SCT lets us know that identities are not just activated when salient but that there are different features to them that can be salient when activated.
    - Fundamentally, SCT is:
      - a person brings ednuring knowledge about the self to a context. The information is a psychological resource in a given situation and can interact with contextual information to produce a self-categorization with associated attitudes and behaviors.
    - Predictions from SCT:
      - People have both a personal and a social identity.
      - People can define and categorize themselves at different levels of abstraction. You can define yourself as an individual, a memberof a particular group in contrast to others, and a member of a higher-order and more inclusive goroup.
      - Salience explains the way a particular situation is categorized and given meaning. The way this self-categorization happens can explain self-perception and behavior.
  }
}




% Theories about responses to threats
%% Overview
@incollection{craig_phillips_2023_ohpp,
  author = {Maureen A. Craig and L. Taylor Phillips},
  year = 2023,
  title = {Group-based Hierarchies of Power and Status},
  booktitle = {The Oxford Handbook of Political Psychology},
  edition = {3rd Edition},
  editors = {Leonie Huddy and David O. Sears and Jack S. Levy and Jennifer Jerit},
  publisher = {Oxford University Press},
  address = {New York},
  annotation = {
    - Provides an excellent literature review of how identity operates and how major theories provides useful predictions about what conditions that attitudes and behavior changes.
    - Major point:
      - merely existing within social hierarchies affects people's psychology and their propensity to engage in political action.
    - Status vs power as concepts:
      - they are theoretically separable. Power indicates control over valued resources and the ability to influence others. Status indicates perceived respect and esteem which can lead to greater influence over others.
    - Realistic conflict theory: suggests that prejudice and conflict stem from competition over scarce resources and group interests.
      - The instrumental model of group conflict: argues that perceptions of resource scarcity and salience of a competitive outgroup guide intergroup competition. Also argues that hierarchy itself facilitiates the perception of limited or scarce resources wheich perpetuates group conflict and hierarchy.
    - Social Dominance Theory:
      - The result of identities
      - suggests that hierarchies emerge due to evolutionary pressures.
      - suggests that individuals interact with hierarchy in one of two ways: hierarchy-enhancing versus hierarchy-attenuating.
      - A central trait predicting social dominance theory is social dominance orientation:
        - it is a fundamental motive underlying hierarchy-enhancing or hierarchy-attenuating behavior.
        - It indicates an orientation towars or preference for group-based dominance and inequality.
        - hierarchy-maintenance outcomes
      - emphasizes hierarchy-enhancing motivations
    - System justification theory:
      - explains why and how hierarchies are preserved once established.
      - Three ways justification occurs:
        - ego justification: want to view oneself positively
        - group-justification: driven by a motive to view one's group positively
        - system-justification: driven by people's desire to view the system or status quo in a favorable light
    - Status characteristics theory
      - sociological perspective on this that compliments the psychological perspectives from SJT and SDT.
      - expectations through group memberships guide status and hierarchies while providing justifications for why they exist.
    - recent work like Kachanoff et al. 2020 demonstrates that white Americans' perceive that other groups are trying to control white Americans. These perceptions predict white Americans' support for working together with others of their racial group.
    - reporting a dominant group identity is central or important to one's sense of self tends to predict support for maintaining or accentuating one's high-statuse place in the hierarchy.
    - work on White racial identity suggests that dominant group members that seek to mitigate the hierarchy may identify with their socially-dominant ingorup but identify in a way that criticizes the existing power structure and perceive their group to possess unearned societal privileges and power.
  }
}
%% Societal threats
@article{marshburn_et-al_2022_gpir,
  author = {Christopher K. Marshburn and Brandon A. Reinkensmeyer and Erick D. Knowles},
  year = 2022,
  title = {Dominance motivated delusions: Whites with high social dominance orientation perceive equal amounts of institutional racism between Blacks and Whites},
  journal = {Group Processes \& Intergroup Relations},
  pages = {1--27},
  doi = {10.1177/13684302221103984},
  annotation = {
    RQ: Why do some Whites percieve whites to be just as discriminated against as Blacks while others don’t?

    Argues:
    - SDO can predict this. Those high in SDO may be extra sensitive to potential threats to the power of Whites in society and they report that Whites are discriminated against more than they are as a way to ensure squash any challenges to the existing racial hierarchy.

    Methods:
    - Study 1a: examines  whether those with higher SDO are more likely to point out instances where Whites are discriminated against as racist and attend less to those that say that an instance discrimination against Blacks was racist.
    - Study 1b: similar to study 1. They assign an example of institutional racism and individual racism per participant to look at within-individual differences on this.
    - Study 2: tries to use less ambiguous individual and more plausible institutional discrimination items.
    
    Findings:
    - Study 1a: those with high levels of SDO are lower on reporting something as institutionally racist when the victim is Black. However, when the victim is white, those high in SDO report higher perceptions in institutional racism than those low on SDO. They find that those low on SDO see the victimizing of subordinate groups as evidence of racism while subordinate groups victimizing dominant groups as not racist. Those with high levels of SDO are opposite of that.
    - Study 1b: those with higher SDO reported that racism was lower for Black victims experiencing institutional and individual discrimination. Whites with higher SDO are resistant to acknowledging racia ldisparities between Blacks and Whites, particularly for institutional forms of discrimination.
    - Study 2: find that those with higher levels of SDO see that Black and White victims report similar levels of institutionalized racism.
  }
}
