%% White identity
@article{knowles_peng_2005_jpsp,
  author = {Eric D. Knowles and Kaiping Peng},
  year = 2005,
  title = {White Selves: Conceptualizing and Measuring a Dominant-Group Identity},
  journal = {Journal of Personality and Social Psychology},
  volume = {89},
  number = {2},
  pages = {223--241},
  doi = {10.1037/0022-3514.89.2.223},
  annotation = {
    RQ: How can we measure white identity?

    Argues:
    - White identity is conceptualized as an automatic association between the self and the white ingroup.
    - Empirical psychology is guilty of the sociological view of whiteness by considering it to be inherently invisible, transparent, or unmarked despite its power to shape lives.
    - Develop a measure of white identity that captures the self-categorization of the not intert and psychologically salient category. Call it the White Identity Centrality Implicit Association test. This measure uses the conceptualization of white identity as the degree to which an individual has incorporated white ingroup membership into their self-concept.
      - It is a measure of the individuals’ hronic self-focus as a white. The higher level their white identity centrality and the more likely they are to shift attention to the self when considering the actions of the ingroup.
      - The individuals that score higher on WICIAT are more likely to experience guilt and negative self-evaluative emotions when made aware of ingroup’s historical transcgressions.

    Methods:
    - studies.

    Findings:
    - Study 1: finds that WICIAT scores were significantly correlated with white ingroup preference IAT scores but not with explicit measures of racial attitudes or white identity centrality. There is also a significnat positive association between home county diversity and WICIAT scores. Compared to those with largely white populations, those in counties with a substantial non-white population show high levels of implicit white identity centraility.
    - Study 2: overall participants divided the black-white continuum near the 50/50 pont. The effect of white ingroup preference IAT score was not significant. However the effect of WICIAT score ws significant such that those with higher WICIAT scores had higher categorization thresholds and excluded more faces from the white ingroup than did low scorers on the WICIAT.
    - Study 3: participants reaction speeds were lower in the middle of the morphing contniuum than at the end points.
    - Study 4
  }
}
@article{knowles_et-al_2014_pps,
  author = {Eric D. Knowles and Brian S. Lowery and Rosalind M. Chow and Miguel M. Unzueta},
  year = 2014,
  title = {Deny, Distance, or Dismantle? How White Americans Manage a Privileged Identity},
  journal = {Perspectives on Psychological Science},
  volume = 9,
  number = 6,
  pages = {594--609},
  doi = {10.1177/1745691614554658},
  annotation = {
    RQ: What are the ways in which Whites tend to interact with their white identity?

    Argues:
    - Historically, the social sciences have often thought that whites are blind to their identity. This is not correct.
    - Whites actively engage with their racial identity and use it to evaluate their world around them.
    - Whites engage with their racial identity in three ways: 
      - deny the existence of privilege
      - distance their own self-concepts from the White category
      - dismantle ystems of privilege
    - Whites tend to default to go with the first two as the third one requires an acknowledge ment of unearned privilege (meritocratic threat) and that they associate with a group that benefits from unfair social advantages (group-image threat). Both of these threats are difficult for whites to accept as it challenges their conception of who they are and the people that are like them.
    - Deny and distancing, the two most common, strategies promote insensitivity and inaction in respect to racial inequality. However, the strategy least common, dismantling, reduces threat by relinquishing privileges.
  }
}
@article{lowery_et-al_2012_jpsp,
  author = {Brian S. Lowery and Rosalind M. Chow and Eric D. Knowles and Miguel M. Unzueta},
  year = 2012,
  title = {Paying for Positive Group Esteem: How Inequality Frames Affect Whites' Responses to Redistributive Policies},
  journal = {Journal of Personality and Social Psychology},
  volume = 102,
  number = 2,
  pages = {323--336},
  doi = {10.1037/a0024598},
  annotation = {
    RQ: Does a frame of unearned white advantage or non-white disadvantage motivate a desire toreduce inequality work better?

    Argues:
    - Social Dominance Theory would suggest that we aren’t likely to see that a frame telling white people that there is a undeserved disadvantage for non-white groups will move them much on a desire to fix that gap. Instead, a frame telling white people that they have unearned privilege will motivate a guilt response for the self and a desire to distance oneself from the group and to respond with support for the idea to make it harder for whites to continue to benefit from this advantage (not support to give non-whites more advantages, just to harm the group that did something bad; it is still entirely about whites and the dominant group).
    
    Methods:
    - 4 studies
    
    Finds:
    - when faced with racial inequality framed as unearned white advantage,  frames of unearned white advantage increases whites’ support for policies perceived to reduce their group’s economic opportunities but not to increase support for policies perceived to increase minority opportunities. These effects do not happen for frames about minority disadvantage. Additionally find that the effect of unearned white advantage frames on whites’ esteem for their ingroup is what drives the effect for the inequality frame on support for policies perceived to reduce whites’ opportunities.
  }
}



